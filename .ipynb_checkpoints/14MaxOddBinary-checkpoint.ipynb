   "source": [
    "\n",
    "def maximumOddBinaryNumber(self, s: str) -> str:\n",
    "    if len(s) == 1:\n",
    "        return s\n",
    "    no1s = 0\n",
    "    for _ in s:\n",
    "        no1s += int(_)\n",
    "    no0s = len(s) - no1s\n",
    "    return (no1s-1)*\"1\" + no0s*\"0\" + \"1\""
   ]
  }
